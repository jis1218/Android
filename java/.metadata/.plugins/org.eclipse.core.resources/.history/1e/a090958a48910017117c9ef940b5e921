/**
 * 클래스 설명
 * @author daeho
 *
 */
public class Sub {
	/**
	 * 메소드 설명
	 */
	public void test(){
		//정수 연산은 int로 대체된다
		byte a = 10;
		byte b = 11;
		
		//byte c = a + b; (x)
		//int c = a + b; (o)
		byte c = (byte) (a + b);
		
		
		long longA = 213984342L;
		
		
		//실수는 모두 double 로 대체된다
		float d = 3.14f;
		float e = 4.1928f;
		
		//실수는 직접연산하지 않는다
		//float f = d + e;  (x)
		float f = Float.sum(d, e);
		
		double g = 5.223;
		double h = 1.98343;
		
		double i = Double.sum(g, h);
	}
}
