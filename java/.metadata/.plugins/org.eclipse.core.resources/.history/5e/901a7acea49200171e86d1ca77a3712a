
public class AlgoMain {

	public static void main(String[] args) {
		
		AlgoMain aMain = new AlgoMain();
		
		long result = aMain.sum(1000000000L);
		System.out.println(result);
	}
	
	/**
	 * 1 ~ max 까지 더하는 함수
	 * @param max
	 * @return
	 */
	public long sum(long max){
		long sum = 0;
		
		for(int i = 1; i <= max; i++) sum += i;
		
		return sum;
	}

}
